    "def get_resids(d):\n",
    "  pca = PCA(1).fit(d.T)\n",
    "  index=pca.components_[0]\n",
    "  mm = [sm.OLS(s, index).fit() for s in d.T]\n",
    "  return list(map(lambda x: x.resid, mm))\n",
    "\n",
    "def Zscore(X):\n",
    "    return np.array((X - np.mean(X)) / np.std(X))"
   ]
  },
